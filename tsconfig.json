{
    "compilerOptions": {
        "module": "commonjs",
        "target": "es5",
        "jsx": "react",
        "esModuleInterop": true,
        "baseUrl": "resources/js",
        "paths": {
            "Components/*": ["Components/*"],
            "Layout/*": ["Components/Layout/*"],
            "Services/*": ["services/*"],
            "Api/*": ["services/Api/*"],
            "*": ["*"],
        }
    },
    "include": [
        "resources/js/**/*"
    ]
}
